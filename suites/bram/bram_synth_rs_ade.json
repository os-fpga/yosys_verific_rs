        {
            "name": "md5_pipelined",
            "rtl_path": "RTL_Benchmark/Verilog/Cores/crypto_core/md5_pipelined/trunk",
            "top_module": "Md5Core"
        },
        {
            "name": "tiny_tate_bilinear_pairing",
            "rtl_path": "RTL_Benchmark/Verilog/OpenCores_designs/tiny_tate_bilinear_pairing/rtl",
            "top_module": "pairing"
        }
